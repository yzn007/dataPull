RD_GATE_EVT  where instr(data_tm ,date_format(DATE_SUB(now(),INTERVAL  1 day),'%Y-%m-%d'))>0;        INSERT INTO CQYL_RD.T80_RD_LARG_SCRN_INDX(RPT_ID,        RPT_NM,        INDX_ID,        INDX_NM,        GRP_ID,        GRP_NM,        INDX_VAL,        STATT_DT) SELECT '101','累计人数统计','105002','已接待预约人数',null,null,COUNT(ID),date_format(now(),'%Y-%m-%d') FROM CQYL_RD.T80_RD_GATE_EVT  where instr(data_tm ,date_format(now(),'%Y-%m-%d'))>0    ;        delete from CQYL_RD.T80_RD_GATE_TMP_EVT;        delete from CQYL_PRE.GATE_GATE_TMP_EVT    ;        INSERT INTO CQYL_RD.T80_RD_GATE_TMP_EVT(ID,        Gate_ID,        Entrc_ID,        Entrc_Nm,        Data_Tm) SELECT T1.USERID,T1.ACCESSPOINT,T2.ACCESSAREA,T2.ACCESSAREA_NM,T1.data_tm FROM CQYL_PRE.GATE_GATE_TMP_EVT T1 LEFT JOIN CQYL_PRE.GATE_AREA_INFO T2 ON T2.ACCESSPOINT=T1.ACCESSPOINT;        DELETE FROM CQYL_RD.T80_RD_LARG_SCRN_INDX WHERE INDX_ID IN('100001','100002') and STATT_DT=date_format(now(),'%Y-%m-%d');        INSERT INTO CQYL_RD.T80_RD_LARG_SCRN_INDX(RPT_ID,        RPT_NM,        INDX_ID,        INDX_NM,        GRP_ID,        GRP_NM,        INDX_VAL,        STATT_DT)        SELECT '100','实时人数统计','100001','登录口实时人数',T1.ACCESSAREA,T1.ACCESSAREA_NM,COUNT(T2.ID), date_format(now(),'%Y-%m-%d')        FROM CQYL_PRE.GATE_AREA_INFO T1        LEFT JOIN CQYL_RD.T80_RD_GATE_TMP_EVT T2        ON T1.ACCESSPOINT=T2.GATE_ID        GROUP BY T1.ACCESSAREA,T1.ACCESSAREA_NM;        INSERT INTO CQYL_RD.T80_RD_GATE_EVT(ID,        Role_Typ,        Role_Typ_Desc,        Gate_ID,        Entrc_ID,        Entrc_Nm,        Trvl_Way,        Data_Tm        )        SELECT T1.ID,COALESCE(T2.ROLETYPE,'-1'),COALESCE(T3.CODE_VAL_DESC,'其他'),T1.GATE_ID,T1.ENTRC_ID,T1.ENTRC_NM,COALESCE(T2.TRAVEL,'-1'),T1.Data_Tm        FROM CQYL_RD.T80_RD_GATE_TMP_EVT T1        LEFT JOIN CQYL_PRE.GATE_EXPO_AUDI_INFO T2 ON T2.ID=T1.ID        LEFT JOIN CQYL_RD.T80_RD_DIC T3 ON T2.ROLETYPE=T3.CODE_VAL AND T3.CODE_ID='10001';        DELETE FROM CQYL_RD.T80_RD_LARG_SCRN_IND