        delete from cqyl_rd.T80_RD_LARG_SCRN_INDX where INDX_ID in( '105001','105003','105004','107001','107002') and STATT_DT=date_format(now(),'%Y-%m-%d');        insert into cqyl_rd.T80_RD_LARG_SCRN_INDX(RPT_ID,        RPT_NM,        INDX_ID,        INDX_NM,        GRP_ID,        GRP_NM,        INDX_VAL,        STATT_DT)        select '105',        '预约观展人数统计',        '105001',        '预约观展人数',        null,        null,        count(g.id),        date_format(now(),'%Y-%m-%d')        from cqyl_pre.GATE_EXPO_AUDI_INFO g        where instr(g.visitDate,DATE_FORMAT(now(),'%Y-%m-%d'))>0;        insert into cqyl_rd.T80_RD_LARG_SCRN_INDX(RPT_ID,        RPT_NM,        INDX_ID,        INDX_NM,        GRP_ID,        GRP_NM,        INDX_VAL,        STATT_DT)        select '105',        '预约观展人数统计',        '105003',        '公共交通出行人数',        null,        null,        count(g.id),        date_format(now(),'%Y-%m-%d')        from cqyl_pre.GATE_EXPO_AUDI_INFO g        where travel IN('1','3') and  instr(g.visitDate,DATE_FORMAT(now(),'%Y-%m-%d'))>0;        insert into cqyl_rd.T80_RD_LARG_SCRN_INDX(RPT_ID,        RPT_NM,        INDX_ID,        INDX_NM,        GRP_ID,        GRP_NM,        INDX_VAL,        STATT_DT)        select '105',        '预约观展人数统计',        '105004',        '自驾出行人数',        null,        null,        count(g.id),        date_format(now(),'%Y-%m-%d')        from cqyl_pre.GATE_EXPO_AUDI_INFO g        where travel ='4'  and instr(g.visitDate,DATE_FORMAT(now(),'%Y-%m-%d'))>0  ;        insert into cqyl_rd.T80_RD_LARG_SCRN_INDX(RPT_ID,        RPT_NM,        INDX_ID,        INDX_NM,        GRP_ID,        GRP_NM,        INDX_VAL,        STATT_DT)        select '107',        '参展人员统计',        '107001',        '安保人员数量',        null,        null,        count(g.id),        date_format(now(),'%Y-%m-%d')        from cqyl_pre.GATE_EXPO_AUDI_INFO g        where g.ROLETYPE='10' and instr(g.visitDate,DATE_FORMAT(now(),'%Y-%m-%d'))>0;        insert into cqyl_rd.T80_RD_LARG_SCRN_INDX(RPT_ID,        RPT_NM,        INDX_ID,        INDX_NM,        GRP_ID,        GRP_NM,        INDX_VAL,        STATT_DT)        select '107',        '参展人员统计',        '107002',        '工作人员数量',        null,        null,        count(g.id),        date_format(now(),'%Y-%m-%d')        from cqyl_pre.GATE_EXPO_AUDI_INFO g        where g.ROLETYPE='2' and instr(g.visitDate,DATE_FORMAT(now(),'%Y-%m-%d'))>0    ;        insert into cqyl_ta.ta_data_upt_msg(update_time,table_name,stat)        select * from (select now() n,lower('T80_TA_EXPO_AUDI_INFO') tb,'0' stat from dual) a WHERE        not EXISTS (select 1 from cqyl_ta.ta_data_upt_msg g where g.update_time = a.n and table_name = a.tb and g.stat = a.stat)    ;

        delete from cqyl_rd.T80_RD_LARG_SCRN_INDX where INDX_ID in( '105001','105003','105004','107001','107002') and STATT_DT=date_format(now(),'%Y-%m-%d');        insert into cqyl_rd.T80_RD_LARG_SCRN_INDX(RPT_ID,        RPT_NM,        INDX_ID,        INDX_NM,        GRP_ID,        GRP_NM,        INDX_VAL,        STATT_DT)        select '105',        '预约观展人数统计',        '105001',        '预约观展人数',        null,        null,        count(g.id),        date_format(now(),'%Y-%m-%d')        from cqyl_pre.GATE_EXPO_AUDI_INFO g        where instr(g.visitDate,DATE_FORMAT(now(),'%Y-%m-%d'))>0;        insert into cqyl_rd.T80_RD_LARG_SCRN_INDX(RPT_ID,        RPT_NM,        INDX_ID,        INDX_NM,        GRP_ID,        GRP_NM,        INDX_VAL,        STATT_DT)        select '105',        '预约观展人数统计',        '105003',        '公共交通出行人数',        null,        null,        count(g.id),        date_format(now(),'%Y-%m-%d')        from cqyl_pre.GATE_EXPO_AUDI_INFO g        where travel IN('1','3') and  instr(g.visitDate,DATE_FORMAT(now(),'%Y-%m-%d'))>0;        insert into cqyl_rd.T80_RD_LARG_SCRN_INDX(RPT_ID,        RPT_NM,        INDX_ID,        INDX_NM,        GRP_ID,        GRP_NM,        INDX_VAL,        STATT_DT)        select '105',        '预约观展人数统计',        '105004',        '自驾出行人数',        null,        null,        count(g.id),        date_format(now(),'%Y-%m-%d')        from cqyl_pre.GATE_EXPO_AUDI_INFO g        where travel ='4'  and instr(g.visitDate,DATE_FORMAT(now(),'%Y-%m-%d'))>0  ;        insert into cqyl_rd.T80_RD_LARG_SCRN_INDX(RPT_ID,        RPT_NM,        INDX_ID,        INDX_NM,        GRP_ID,        GRP_NM,        INDX_VAL,        STATT_DT)        select '107',        '参展人员统计',        '107001',        '安保人员数量',        null,        null,        count(g.id),        date_format(now(),'%Y-%m-%d')        from cqyl_pre.GATE_EXPO_AUDI_INFO g        where g.ROLETYPE='10' and instr(g.visitDate,DATE_FORMAT(now(),'%Y-%m-%d'))>0;        insert into cqyl_rd.T80_RD_LARG_SCRN_IND