<?xml version="1.0" encoding="UTF-8"?>
<root>

    <!-- 模板配置 -->
    <!-- 子表配置参数 id:数据库字段名 ref:json字段名 [list]:子表list表名 linkId:关联主表字段 条件值=左边数据库字段，右边json即【ref】字段-->
    <!-- 用户信息 -->
    <table name="ODS.PUB_UNIFIED_IDENTITY_USER">
        <column id="id" ref="systemUserId">id = systemUserId</column>
        <column id="username" ref="_user"/>
        <column id="organization" ref="_organization"/>
        <column id="user_name" ref="username"/>
        <!--<column id="" ref="password"/>-->
        <column id="full_name" ref="fullname"/>
        <column id="is_disabled" ref="isDisabled"/>
        <column id="is_locked" ref="isLocked"/>
        <column id="create_at" ref="createAt"/>
        <column id="update_at" ref="updateAt"/>
        <column id="is_system" ref="isSystem"/>
        <column id="is_public" ref="isPublic"/>
        <column id="is_master" ref="isMaster"/>
        <!--<column id="" ref="email"/>-->
        <column id="employee_id" ref="employeeNo"/>
        <column id="phone" ref="mobile"/>
        <column id="effect_on" ref="effectOn"/>
        <!--<column id="" ref="sex"/>-->
        <!--<column id="" ref=""/>-->
        <!--<column id="" ref=""/>-->
        <!--<column id="" ref=""/>-->
    </table>

    <!-- 机构信息 -->
    <table name="ODS.PUB_UNIFIED_IDENTITY_ORG">
        <column id="id" ref="systemOrgId">id = systemOrgId</column>
        <column id="parent_Id" ref="_parent"/>
        <column id="org_name" ref="_organization"/>
        <column id="full_name" ref="fullname"/>
        <column id="org_full_name" ref="orgFullName"/>
        <column id="is_disabled" ref="isDisabled"/>
        <column id="effect_on" ref="effectOn"/>
        <column id="create_at" ref="createAt"/>
        <column id="update_at" ref="updateAt"/>
        <column id="address" ref="linkAddress"/>
    </table>

    <!-- 设备信息 -->
    <!-- 服务器-->
    <table name="ODS.O_A00_50_SXF_DEV_SERVER_INFO">
        <column id="server_id" ref="device_id">server_id = device_id</column>
        <column id="clients" ref="[list]ODS.O_A00_50_SXF_DEV_CLIENT_INFO" linkId="server_id"/>
        <column id="name" ref="device_name"/>
    </table>
    <!-- 客户端信息 -->
    <table name="ODS.O_A00_50_SXF_DEV_CLIENT_INFO">
        <column id="id" ref="device_id">id = device_id</column>
        <!--<column id="server_id" ref="serverId"/>-->
        <column id="name" ref="device_name"/>
    </table>

    <!-- bbc机构信息 -->
    <table name="ODS.O_A00_50_SXF_BBC_ORG_INFO">
        <column id="id" ref="org_id">id = org_id</column>
        <column id="parent_id" ref="parent_org_id"/>
        <column id="name" ref="org_name"/>
    </table>

    <!-- bbc设备列表 -->
    <table name="ODS.O_A00_50_SXF_DEV_LIST_INFO">
        <column id="device_id" ref="device_id">device_id = device_id</column>
    </table>
</root>





